    <!ELEMENT countries (country*)>
    <!ELEMENT country (city*, language*)>
    <!ATTLIST country 
            name CDATA #REQUIRED 
            population CDATA #REQUIRED 
            area CDATA #REQUIRED>
    <!ELEMENT city (name, population) >
    <!ELEMENT name ANY>
    <!ELEMENT population ANY>
    <!ELEMENT language ANY>
    <!ATTLIST language percentage CDATA #IMPLIED >


